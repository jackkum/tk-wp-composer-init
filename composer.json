    "wpackagist-plugin/cyr3lat": "~3.5",
    "wpackagist-plugin/easy-wp-smtp": "*",
    "wpackagist-plugin/gd-bbpress-attachments": "~2.4",
    "wpackagist-plugin/google-sitemap-generator": "*",
    "wpackagist-plugin/jetpack": "~4.3.1",
