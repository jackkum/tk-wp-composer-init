    "wpackagist-plugin/wp-pagenavi": "~2.91",
    "wpackagist-plugin/wp-postviews": "~1.73",
    "wpackagist-plugin/wp-recaptcha-integration": "~1.1.10",
    "wpackagist-plugin/wp-ulike": "~2.4",
    "wpackagist-plugin/wpfront-user-role-editor": "~2.12.4",

    "wpackagist-theme/twentythirteen":"*",
    "wpackagist-theme/holi":"*"
